{
  "name": "generator-yoyomern",
  "version": "0.1.1",
  "description": "This creates a file structure using Mongo, React, Express and Node",
  "homepage": "https://github.com/jsrice7391/yeomanMERN",
  "author": {
    "name": "Justin Rice",
    "email": "jsrice7391@gmail.com",
    "url": "http://jsrice7391.github.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "MERN",
    "Mongo",
    "React",
    "Express",
    "Node",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^4.2.1",
    "nsp": "^3.2.1",
    "prettier": "^1.14.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "npm": "^6.4.0",
    "snyk": "^1.124.1",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "jsrice7391/generator-mern-stack-generator",
  "license": "unlicense",
  "snyk": true
}
